<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/MainButtonDef[defName = "Research"]/tabWindowClass</xpath>
    <value>
      <tabWindowClass>OrganizedResearch.OrganizedResearch</tabWindowClass>
    </value>
  </Operation>
  
  <Operation Class="PatchOperationReplace">
	<xpath>/Defs/ResearchProjectDef/tab</xpath>
	<value>
		<tab>Main</tab>
	</value>
  </Operation>
  
  <Operation Class="PatchOperationRemove">
	<xpath>/Defs/ResearchTabDef[defName!="Main"]</xpath>
  </Operation>
</Patch>